
{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      
     
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "/https://medicos-frontend.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
    }
  ]
}